    "start3": "nodemon ./src/app",
    "dev": "NODE_ENV=dev nodemon   ./src/app  --exec babel-node",
    "una": "NODE_ENV=una nodemon   ./src/app  --exec babel-node",
    "rtest": "NODE_ENV=test nodemon   ./src/app  --exec babel-node",
    "local": "NODE_ENV=local nodemon   ./src/app  --exec babel-node",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel & scp -r /home/simon/sys/apiv4/build  devs@m.unabase.cl:/home/devs/api/",
    "sbuild": "npm run clean && npm run build-babel",
    "start": "npm run build-babel && node ./build/app.js"
  },

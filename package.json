  "scripts": {
    "btest": "npm run build && NODE_ENV=test mocha-parallel-tests --exit --timeout 20000",
    "test": "NODE_ENV=dev mocha-parallel-tests --exit --timeout 20000 --require  babel-core/register --exclude /modules --require @babel/polyfill",
    "testl": "NODE_ENV=local mocha-parallel-tests --exit --timeout 20000 --require  babel-core/register --exclude /modules --require @babel/polyfill",
    "tdev": "NODE_ENV=dev mocha-parallel-tests --exit --timeout 20000 --require  babel-core/register --exclude /modules --require @babel/polyfill --grep",
